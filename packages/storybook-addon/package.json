{
  "name": "@urql/storybook-addon",
  "version": "1.0.0",
  "description": "Make stories in Storybook with urql",
  "sideEffects": false,
  "homepage": "https://github.com/FormidableLabs/urql/tree/main/packages/storybook-addon",
  "bugs": "https://github.com/FormidableLabs/urql/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/urql.git",
    "directory": "packages/storybook-addon"
  },
  "keywords": [
    "addon",
    "storybook",
    "urql",
    "formidablelabs",
    "preact",
    "react"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "check": "tsc --noEmit",
    "lint": "eslint --ext=js,jsx,ts,tsx .",
    "build": "rollup -c ../../scripts/rollup/config.js && cp src/storybook.d.ts dist"
  },
  "devDependencies": {
    "@urql/preact": "latest",
    "@storybook/preact": ">= 6.0.0",
    "@storybook/react": ">= 6.0.0",
    "preact": "^10.4.8",
    "react": "^16.13.1",
    "urql": "latest"
  },
  "peerDependencies": {
    "@storybook/addons": ">= 6.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
